{
  "kind": "collectionType",
  "collectionName": "blogs",
  "info": {
    "name": "blog"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "Title": {
      "type": "string",
      "required": true
    },
    "Image": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": true
    },
    "Slug": {
      "type": "string",
      "required": true
    },
    "Category": {
      "type": "enumeration",
      "enum": [
        "JavaScript",
        "HTML",
        "CSS",
        "SASS",
        "React",
        "Vue"
      ],
      "required": true
    },
    "Description": {
      "type": "text",
      "required": true
    },
    "Content": {
      "type": "richtext",
      "required": true
    }
  }
}
